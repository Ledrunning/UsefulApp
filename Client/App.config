            <endpoint address="net.tcp://127.0.0.1:6002/Icontract" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_INoteServiceContract" contract="NotesService.INoteServiceContract" name="NetTcpBinding_INoteServiceContract">
                <identity>
                    <userPrincipalName value="OSMAN_MAZINOV\Mazinov"/>
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>
